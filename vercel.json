{
  "version": 2,
  "public": true,
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.sharepoint.com https://sharepoint.com;"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://*.sharepoint.com"
        }
      ]
    }
  ]
}
