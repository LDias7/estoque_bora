{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.sharepoint.com https://sharepoint.com;"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://*.sharepoint.com"
        }
      ]
    }
  ]
}
